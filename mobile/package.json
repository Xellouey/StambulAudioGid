{
  "name": "dagestan-audio-guide-mobile",
  "version": "1.0.0",
  "description": "Mobile app for Dagestan Audio Guide",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --run",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace DagestanAudioGuide.xcworkspace -scheme DagestanAudioGuide -configuration Release -destination generic/platform=iOS -archivePath DagestanAudioGuide.xcarchive archive"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "react-native-screens": "^3.22.1",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-vector-icons": "^10.0.0",
    "react-native-maps": "^1.7.1",
    "react-native-yandex-mapkit": "^1.0.0",
    "react-native-sound": "^0.11.2",
    "react-native-iap": "^12.10.7",
    "react-native-device-info": "^10.8.0",
    "react-native-async-storage": "^1.19.1",
    "@react-native-async-storage/async-storage": "^1.19.1",
    "react-native-image-picker": "^5.6.0",
    "react-native-permissions": "^3.8.4",
    "axios": "^1.4.0",
    "react-query": "^3.39.3",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "@types/react-native-vector-icons": "^6.4.13"
  },
  "engines": {
    "node": ">=16"
  }
}